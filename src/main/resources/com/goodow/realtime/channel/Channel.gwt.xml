<?xml version="1.0" encoding="UTF-8"?>
<module>
  <inherits name='com.goodow.realtime.operation.Operation'/>
  
  <replace-with class="com.goodow.realtime.channel.http.js.JsXDomainRequestBuilder">
    <when-type-is class="com.google.gwt.http.client.RequestBuilder"/>
    <any>
      <when-property-is name="user.agent" value="ie8" />
      <when-property-is name="user.agent" value="ie9" />
    </any>
  </replace-with>
  
  <source path="">
    <exclude name="util/impl/JreChannelFactory.java" />
    <exclude name="util/impl/ObjCChannelFactory.java" />
  </source>
  <super-source path="super"/>
</module>